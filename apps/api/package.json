{
  "name": "moneyshyft",
  "version": "0.3.6",
  "description": "Family-focused budgeting application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate:latest": "node -r ts-node/register ./node_modules/knex/bin/cli.js --knexfile src/knexfile.ts migrate:latest",
    "migrate:latest:test": "DOTENV_CONFIG_PATH=.env.test node -r dotenv/config -r ts-node/register ./node_modules/knex/bin/cli.js --knexfile src/knexfile.ts migrate:latest",
    "migrate:latest:prod": "node ./node_modules/knex/bin/cli.js --knexfile dist/knexfile.js migrate:latest",
    "migrate:rollback": "node -r ts-node/register ./node_modules/knex/bin/cli.js --knexfile src/knexfile.ts migrate:rollback",
    "migrate:rollback:test": "DOTENV_CONFIG_PATH=.env.test node -r dotenv/config -r ts-node/register ./node_modules/knex/bin/cli.js --knexfile src/knexfile.ts migrate:rollback",
    "migrate:rollback:prod": "node ./node_modules/knex/bin/cli.js --knexfile dist/knexfile.js migrate:rollback",
    "migrate:make": "node -r ts-node/register ./node_modules/knex/bin/cli.js --knexfile src/knexfile.ts migrate:make",
    "seed:run": "node -r ts-node/register ./node_modules/knex/bin/cli.js --knexfile src/knexfile.ts seed:run",
    "seed:run:test": "DOTENV_CONFIG_PATH=.env.test node -r dotenv/config -r ts-node/register ./node_modules/knex/bin/cli.js --knexfile src/knexfile.ts seed:run",
    "seed:run:prod": "node ./node_modules/knex/bin/cli.js --knexfile dist/knexfile.js seed:run",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "@moneyshyft/shared": "workspace:*",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "uuid": "^8.3.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
